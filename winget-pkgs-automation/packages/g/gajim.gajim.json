{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "Gajim.Gajim",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://gajim.org/current-version.json",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "1.4.5",
  "ManifestFields": {
    "PackageVersion": "($Response.current_version | Select-String -Pattern $VersionRegex).Matches.Value",
    "InstallerUrls": "$Response.current_win_setup",
    "ReleaseDate": "(Get-Date -Date ((Invoke-RestMethod -Uri https://dev.gajim.org/gajim/gajim/-/raw/master/ChangeLog | Select-String -Pattern \"Gajim $($Response.current_version) \\((.*)\\)\").matches.Groups[1].Value)).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}